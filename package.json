{
  "name": "umi-ts-cli",
  "version": "0.0.1",
  "description": "Umi + Typescript 脚手架",
  "repository": "git@github.com:didilinkin/umi-ts-cli.git",
  "author": "yanxiaodi <929213769@qq.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env FORK_TS_CHECKER=1 PORT=8001 umi dev",
    "build": "cross-env umi build",
    "test": "cross-env umi test",
    "lint:es": "cross-env eslint --ext .js src mock tests",
    "lint:ts": "cross-env tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "precommit": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\""
  },
  "dependencies": {
    "@antv/g2": "^3.4.10",
    "ant-design-pro": "^2.2.1",
    "antd": "^3.13.2",
    "cross-env": "^5.2.0",
    "dva": "^2.5.0-beta.2",
    "dva-core": "^1.4.0",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "memoize-one": "^5.0.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^3.0.0",
    "polished": "^2.3.3",
    "qs": "^6.6.0",
    "rc-drawer-menu": "^1.1.0",
    "react": "^16.6.3",
    "react-document-title": "^2.0.3",
    "styled-components": "^4.1.3",
    "umi": "^2.4.4",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.7.0",
    "redbox-react": "^1.6.0",
    "redux": "3.7.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "umi-plugin-react": "^1.4.0"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
